group "0.sim" {

  cmd "0.kuka_simulation" {
    exec = "$SPARTAN_BUILD_DIR/drake/drake/examples/kuka_iiwa_arm/kuka_simulation";
    host = "localhost";
  }

  cmd "1.plan-runner" {
    exec = "$SPARTAN_BUILD_DIR/drake/drake/examples/kuka_iiwa_arm/kuka_plan_runner";
    host = "localhost";
  }

  cmd "2.state-translator" {
    exec = "directorPython $SPARTAN_SOURCE_DIR/apps/iiwa/kuka_iiwa_state_translator.py";
    host = "localhost";
  }
}

cmd "4.director" {
  exec = "kuka_iiwa_app";
  host = "localhost";
}
