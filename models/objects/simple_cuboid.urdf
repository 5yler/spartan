<?xml version="1.0"?>
<robot name="simple_cuboid">

  <link name="cube">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.00541" ixy="0" ixz="0" iyy="0.004166" iyz="0"
          izz="0.020833" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.07 0.07 0.07" />
      </geometry>
      <material>
        <color rgba="0.6 0.6 0.6 1" />
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.07 0.07 0.07" />
      </geometry>
    </collision>
   </link>

  <link name="cube2">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.00541" ixy="0" ixz="0" iyy="0.004166" iyz="0"
          izz="0.020833" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.07 0.07 0.07" />
      </geometry>
      <material>
        <color rgba="0.6 0.6 0.6 1" />
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.07 0.07 0.07" />
      </geometry>
    </collision>
   </link>

  <link name="base_link">
  </link>

  <link name="base_link2">
  </link>

  <joint name="cube_to_base" type="fixed">
   <origin xyz="0.0 0 0.1" rpy="0.0 0.0 0.3"/>
   <parent link="base_link"/>
   <child link="cube"/>
  </joint>

  <joint name="cube2_to_base2" type="fixed">
   <origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.3"/>
   <parent link="base_link2"/>
   <child link="cube2"/>
  </joint>


</robot>
